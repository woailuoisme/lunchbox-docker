APP_ENV=fpm
APP_PATH=/var/www/lunchbox
TIMEZONE=Asia/Shanghai

CHANGE_SOURCE=true
# Point to the path of your applications code on your host
APP_CODE_PATH_HOST=../
# Point to where the `APP_CODE_PATH_HOST` should be in the container
APP_CODE_PATH_CONTAINER=/var/www
# You may add flags to the path `:cached`, `:delegated`. When using Docker Sync add `:nocopy`
APP_CODE_CONTAINER_FLAG=:cached

APP_CODE_PATH=/var/www

DATA_PATH=./data/
CONFIG_PATH=./
LOG_PATH=./logs/

### CERTBOT ###############################################
DOMAIN=example.com
# DNS 提供商: cloudflare 或 aliyun
CERTBOT_DNS_PROVIDER=aliyun
# 证书通知邮箱
CERTBOT_EMAIL=admin@example.com
# 续签检查间隔(秒)
CERTBOT_RENEW_INTERVAL=43200

# 阿里云 DNS API 凭证
DNS_ALIYUN_ACCESS_KEY=
DNS_ALIYUN_ACCESS_KEY_SECRET=

# Cloudflare DNS API 凭证
DNS_CLOUDFLARE_API_TOKEN=

### PHP-FPM ###############################################
PHP_VERSION=8.4
PHP_UPSTREAM_CONTAINER=php-fpm
PHP_UPSTREAM_PORT=9000

# 容器用户配置 - 统一使用 www-data 用户(UID=33/GID=33)
WWWUSER=33
WWWGROUP=33
WWW_DATA_PASS=

# Container startup control
ENABLE_SUPERVISOR=true
SUPERVISOR_PHP_USER=www-data
SUPERVISOR_PHP_COMMAND=php-fpm

# Xdebug 配置 https://xdebug.org/docs/all_settings#mode
XDEBUG_MODE=off

# 站点域名
SITE_ADDRESS=example.com

### REDIS #################################################
REDIS_VERSION=8.4-alpine
REDIS_PASSWORD=
REDIS_PORT=6379

### POSTGRES ##############################################
POSTGRES_VERSION=17.7-alpine3.23
POSTGRES_PORT=5432
POSTGRES_DB=lunchbox
POSTGRES_USER=postgres
POSTGRES_PASSWORD=

### MINIO #################################################
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=
MINIO_PORT=9526
MINIO_PORT_UI=9527

### NGINX #################################################
NGINX_VERSION=1.29-alpine
NGINX_HOST_HTTP_PORT=80
NGINX_HOST_HTTPS_PORT=443
NGINX_HOST_LOG_PATH=./logs/nginx/
NGINX_SITES_PATH=./nginx/sites/
NGINX_SSL_PATH=./nginx/ssl
NGINX_PHP_UPSTREAM_CONTAINER=php-fpm
NGINX_PHP_UPSTREAM_PORT=9000

### RABBITMQ ##############################################
RABBITMQ_VERSION=4.1-management-alpine
RABBITMQ_PORT=5672
RABBITMQ_DEFAULT_USER=rabbitmq
RABBITMQ_DEFAULT_PASS=
RABBITMQ_DEFAULT_VHOST=app-host

### MEILISEARCH ###########################################
MEILI_VERSION=v1.30
MEILI_PORT=7700
MEILI_MASTER_KEY=

### GOTIFY ################################################
GOTIFY_DEFAULTUSER_NAME=admin
GOTIFY_DEFAULTUSER_PASS=

### AUTHELIA ##############################################
RESEND_API_KEY=
