[program:horizon]
environment=APP_ENV="docker",APP_DEBUG="false",APP_PATH="/var/www/lunchbox",PHP_PATH="/usr/local/bin"
process_name = %(program_name)s_%(process_num)s
command = /usr/local/bin/php /var/www/lunchbox/artisan horizon
autostart = true
autorestart = true
stdout_logfile = /dev/stdout
stdout_logfile_maxbytes = 0
stderr_logfile = /dev/stderr
stderr_logfile_maxbytes = 0
stopwaitsecs = 3600